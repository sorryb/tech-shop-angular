{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/food-shop-ngrx/src",
  "prefix": "food-shop-architecture-workshop",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/food-shop-ngrx",
        "index": "apps/food-shop-ngrx/src/index.html",
        "main": "apps/food-shop-ngrx/src/main.ts",
        "polyfills": "apps/food-shop-ngrx/src/polyfills.ts",
        "tsConfig": "apps/food-shop-ngrx/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/food-shop-ngrx/src/favicon.ico",
          "apps/food-shop-ngrx/src/assets",
          {
            "glob": "**/*",
            "input": "libs/core/theme/assets/icons/",
            "output": "assets/icons/"
          },
          {
            "glob": "**/*",
            "input": "libs/core/theme/assets/food/",
            "output": "assets/food/"
          }
        ],
        "styles": [
          "libs/core/theme/styles/styles.scss",
          "apps/food-shop-ngrx/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/food-shop-ngrx/src/environments/environment.ts",
              "with": "apps/food-shop-ngrx/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "food-shop-ngrx:build:production"
        },
        "development": {
          "browserTarget": "food-shop-ngrx:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202,
        "proxyConfig": "proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "food-shop-ngrx:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/food-shop-ngrx/**/*.ts",
          "apps/food-shop-ngrx/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/food-shop-ngrx"
      ],
      "options": {
        "jestConfig": "apps/food-shop-ngrx/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
